#include "make_uno.h"
#include <string>
//# is unary -
//$ is unary +
TEST(Uno, FirstTest) {
    string test_str = "-1";
    make_uno(test_str);
    EXPECT_EQ(test_str, "#1");
}

TEST(Uno, SecondTest) {
string test_str = "+1";
make_uno(test_str);
EXPECT_EQ(test_str, "$1");
}

TEST(Uno, ThirdTest) {
string test_str = "1-+3(-a+b)";
make_uno(test_str);
EXPECT_EQ(test_str, "1-$3(#a+b)");
}

